<div class="container">
  <div class="row">
    <div class="offset-md-1">
      <%= form_with model: @user, url: user_user_path, method: :patch do |f| %>
        <h2>会員情報編集</h2>
        <%= f.text_field :name %>
        <%= f.email_field :email %>
        <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
        <%= f.submit class: 'btn btn-secondary' %>
      <% end %>
    </div>
  </div>
</div>
